{
  "private": true,
  "scripts": {
    "start": "cross-env PORT=8001 UMI_ENV=dev umi dev",
    "build:dev": "cross-env UMI_ENV=dev umi build",
    "build:sit": "cross-env UMI_ENV=sit umi build",
    "build:uat": "cross-env UMI_ENV=uat umi build",
    "build:uat1": "cross-env UMI_ENV=uat1 umi build",
    "build:prd": "cross-env UMI_ENV=prd umi build",
    "build:prd1": "cross-env UMI_ENV=prd1 umi build",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@antv/g2": "^4.2.5",
    "@antv/l7": "^2.9.16",
    "@antv/l7-maps": "^2.9.16",
    "@types/qs": "^6.9.7",
    "ahooks": "^3.6.2",
    "antd": "^5.4.0",
    "antd-mobile": "^5.29.0",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "echarts": "^5.3.3",
    "js-cookie": "^3.0.1",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "qs": "^6.11.0",
    "react": "17.x",
    "react-dom": "17.x",
    "react-router-dom": "^6.3.0",
    "umi": "^3.5.30"
  },
  "devDependencies": {
    "@types/js-md5": "^0.4.3",
    "@types/lodash": "^4.14.182",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/lint": "^4.0.7",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.30",
    "eslint": "^8.20.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "stylelint": "^14.9.1",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
